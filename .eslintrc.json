{
  "extends": "standard",
  "parserOptions": {
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "react/jsx-uses-vars": 1,
    "react/jsx-uses-react": "error",
    "react/react-in-jsx-scope": "error"
  },
    "env": {
        "browser": true
    }
}
